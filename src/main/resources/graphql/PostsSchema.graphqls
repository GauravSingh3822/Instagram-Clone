type Posts {
    id: ID!
    title: String
    content: String
    image: String
    createdAt: String
    updatedAt: String
    deletedAt: String
    isDeleted: Boolean
    likesCount: Int
    commentsCount: Int
    isArchived: Boolean
    isReported: Boolean
    isSaved: Boolean
    isLiked: Boolean
    isBookmarked: Boolean
    user: UserEntity!    # Relation: Each post belongs to one user
}